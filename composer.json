{
    "name": "sebastianjung/laravel-vault-423",
    "description": "A Customizable Password Protection Middleware For Laravel Applications That Fits Your Brand.",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Sebastian Jung",
        "email": "jungsebastian92@gmail.com"
    }],
    "require": {
        "php": ">=7.3",
        "laravel/framework": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "SebastianJung\\Vault423\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SebastianJung\\Vault423\\Vault423ServiceProvider"
            ]
        }
    },
    "post-update-cmd": [
        "php artisan clear-compiled",
        "php artisan optimize"
    ]
}