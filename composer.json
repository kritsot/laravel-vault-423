{
    "name": "sebastianjung/laravel-vault-423",
    "description": "A Laravel Password Protection Middleware",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Jung",
            "email": "jungsebastian92@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": ">=5.3.0"
    },
    "autoload": {
        "psr-4": {
            "SebastianJung\\Vault423\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SebastianJung\\Vault423\\Vault423ServiceProvider"
            ]
        }
    },
    "post-update-cmd": [
        "php artisan clear-compiled",
        "php artisan optimize",
        "php artisan vendor:publish --provider='SebastianJung\\Vault423\\Vault423ServiceProvider'"
    ]
}
